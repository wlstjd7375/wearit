<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/round_angle_d3d3d3"
    android:padding="2dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_angle_ffffff"
        android:orientation="vertical"
        android:paddingTop="12dp"
        android:paddingLeft="18dp"
        android:paddingRight="18dp"
        android:paddingBottom="18dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_code"
            android:textSize="13sp"
            android:textStyle="bold"
            android:textColor="#009688"
            android:includeFontPadding="false"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:layout_marginTop="7dp">
            <RelativeLayout
                android:layout_width="32dp"
                android:layout_height="16dp"
                android:background="#808080"
                android:id="@+id/rl_doro_guide"
                android:layout_centerVertical="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9sp"
                    android:textColor="#FFFFFF"
                    android:text="도로명"
                    android:layout_centerInParent="true"
                    android:includeFontPadding="false"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:layout_toRightOf="@+id/rl_doro_guide"
                android:layout_toEndOf="@+id/rl_doro_guide"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_doro"
                    android:textSize="11sp"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:layout_centerVertical="true"
                    android:includeFontPadding="false"/>
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:background="#FFFFFF">
            <RelativeLayout
                android:layout_width="32dp"
                android:layout_height="16dp"
                android:background="#808080"
                android:id="@+id/rl_jibun_guide"
                android:layout_centerVertical="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9sp"
                    android:textColor="#FFFFFF"
                    android:text="지번"
                    android:layout_centerInParent="true"
                    android:includeFontPadding="false"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:layout_toRightOf="@+id/rl_jibun_guide"
                android:layout_toEndOf="@+id/rl_jibun_guide"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_jibun"
                    android:textSize="11sp"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:layout_centerVertical="true"
                    android:includeFontPadding="false"/>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>